<?xml version="1.0" encoding="UTF-8"?>
<Patch>

  <Operation Class="PatchOperationAdd">
    <xpath>/Defs/ThingDef[defName="ToolCabinet"]/comps</xpath>
    <value>
		<li Class="EvenMoreLinkables.CompProperties_QualityOffset">
			<qualityOffset>1</qualityOffset>
		</li>
    </value>
  </Operation>
  
	<Operation Class="PatchOperationAddModExtension">
		<xpath>Defs/ThingDef[defName = "MeleeWeapon_Knife"]</xpath>
		<value>
			<li Class = "EvenMoreLinkables.RequiredLinkablesToCraft">
				<requiredLinkables>
					<li>ToolCabinet</li>
				</requiredLinkables>
			</li>
		</value>
	</Operation>

</Patch>